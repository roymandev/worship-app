{
  "name": "template-react",
  "version": "0.0.0",
  "description": "My template to make react project",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --config ./.eslintrc.json ./src/**/*.{tsx,ts}",
    "format": "prettier --config ./.prettierrc.json -w ./src/**/*.{tsx,ts}",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "firebase": "^9.9.4",
    "jotai": "^1.8.2",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-split": "^2.0.14",
    "tailwind-merge": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --write"
    ]
  }
}
